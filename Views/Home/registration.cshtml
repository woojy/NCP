@model metoo.Models.CUser

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>후보등록</title>
    <link rel="stylesheet" href="~/StyleSheet1.css" />
</head>
<body>
    <div>
        <h2>
            <a href="@Url.Action("index", "Home")">
                홈으로 돌아가기
            </a>
        </h2>
        @using (Html.BeginForm("Registration", "Home"))
        {

        <table class="AdminTable">
            <tr>
                <td>이름</td>
                <td>@Html.TextBoxFor(x => x.name, new { size = "20" })</td>
            </tr>
            <tr>
                <td>학과</td>
                <td>@Html.TextBoxFor(x => x.major, new { })</td>
            </tr>
            <tr>
                <td>공약 1:</td>
                <td>@Html.TextBoxFor(x => x.promise1, new { })</td>
            </tr>
            <tr>
                <td>공약 2:</td>
                <td>@Html.TextBoxFor(x => x.promise2, new { })</td>
            </tr>
            <tr> 
                <td>공약 3:</td>
                <td>@Html.TextBoxFor(x => x.promise3, new { })</td>
            </tr>
        </table>
            <input type="submit" class="buttonInput" name="name" value="등록하기" />
            <!--<h2><a href="@Url.Action("Admin","")">HOME</a></h2>-->
        }
    </div>
</body>
</html>